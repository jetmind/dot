[color]
  ui = true

[alias]
  co = checkout
  c = checkout
  ci = commit
  st = status
  b = branch
  df = diff
  dfc = diff --cached
  l = log --decorate --stat --graph --relative-date
  ll = log --decorate --stat --graph --patch
  l1 = log --pretty=oneline --graph --abbrev-commit
  pushm = push origin master

[push]
  default = current

[diff]
  # follow renames when diffing
  renames = true
  # don't show a/ and b/ prefixes in front of file paths
  noprefix = true

[branch]
  autosetupmerge = true
